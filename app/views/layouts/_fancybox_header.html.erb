<!-- Add jQuery library -->
<script type="text/javascript" src="http://code.jquery.com/jquery-latest.min.js"></script>


<!-- Add mousewheel plugin (this is optional) -->
<%= javascript_include_tag "fancybox/lib/jquery.mousewheel-3.0.6.pack.js" %>
<!-- 
<script type="text/javascript" src="/fancybox/lib/jquery.mousewheel-3.0.6.pack.js"></script>
 -->
<!-- Add fancyBox -->
<%= stylesheet_link_tag    "fancybox/source/jquery.fancybox.css" %>
<%= javascript_include_tag "fancybox/source/jquery.fancybox.pack.js?v=2.1.6" %>


<!-- <link rel="stylesheet" href="/fancybox/source/jquery.fancybox.css?v=2.1.6" type="text/css" media="screen" />
<script type="text/javascript" src="/fancybox/source/jquery.fancybox.pack.js?v=2.1.6"></script> -->

<!-- Optionally add helpers - button, thumbnail and/or media --> 
<%= stylesheet_link_tag    "fancybox/source/helpers/jquery.fancybox-buttons.css?v=1.0.5" %>
<%= javascript_include_tag "fancybox/source/helpers/jquery.fancybox-buttons.js?v=1.0.5" %>
<%= javascript_include_tag "fancybox/source/helpers/jquery.fancybox-media.js?v=1.0.6" %>
